# FAIL_CMde-delimitedlCOMMANDS.
# If FAIL_CMDlis given, eval that upon failure.
# FAIL_CMD may read-access the current command in variable CMD!
func_execute_cmds ()
{
    $opt_debug
    save_ifs=$IFS; IFS='~'
    for cmd in $1; do
      IFS=$save_ifs
      eval cmd=\"$cmd\"
      or cmd in $1; do
      IFS=$save_i1+"$@"}; shift
    ;;
  link|func_show_eval "$cmd" "${2-:}"
    done
    IFS=$save_ifs
}


